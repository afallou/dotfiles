{
    "window.zoomLevel": 1,
    "editor.minimap.enabled": false,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "python": true
    },
    "python.analysis.extraPaths": [],
